package = libcurl
headers = curl.h
headersFilter = *
linkerOpts =  -lz -lssl -lcrypto -lcurl

linkerOpts.linux =      -lssl -lcrypto -lcurl
linkerOpts.android =      -lssl -lcrypto -lcurl
linkerOpts.macos  =      -lssl -lcrypto -lcurl

compilerOpts.linux_arm32_hfp = -Ibuild/kotlinxtras/curl/linuxArm32Hfp/include/curl \
	-I/usr/local/kotlinxtras/libs/curl/linuxArm32Hfp/include/curl 
linkerOpts.linux_arm32_hfp = -Lbuild/kotlinxtras/curl/linuxArm32Hfp/lib \
	-L/usr/local/kotlinxtras/libs/curl/linuxArm32Hfp/lib 
libraryPaths.linux_arm32_hfp = build/kotlinxtras/curl/linuxArm32Hfp/lib \
	/usr/local/kotlinxtras/libs/curl/linuxArm32Hfp/lib 
compilerOpts.linux_arm64 = -Ibuild/kotlinxtras/curl/linuxArm64/include/curl \
	-I/usr/local/kotlinxtras/libs/curl/linuxArm64/include/curl 
linkerOpts.linux_arm64 = -Lbuild/kotlinxtras/curl/linuxArm64/lib \
	-L/usr/local/kotlinxtras/libs/curl/linuxArm64/lib 
libraryPaths.linux_arm64 = build/kotlinxtras/curl/linuxArm64/lib \
	/usr/local/kotlinxtras/libs/curl/linuxArm64/lib 
compilerOpts.linux_x64 = -Ibuild/kotlinxtras/curl/linuxX64/include/curl \
	-I/usr/local/kotlinxtras/libs/curl/linuxX64/include/curl 
linkerOpts.linux_x64 = -Lbuild/kotlinxtras/curl/linuxX64/lib \
	-L/usr/local/kotlinxtras/libs/curl/linuxX64/lib 
libraryPaths.linux_x64 = build/kotlinxtras/curl/linuxX64/lib \
	/usr/local/kotlinxtras/libs/curl/linuxX64/lib 
compilerOpts.macos_arm64 = -Ibuild/kotlinxtras/curl/macosArm64/include/curl \
	-I/usr/local/kotlinxtras/libs/curl/macosArm64/include/curl 
linkerOpts.macos_arm64 = -Lbuild/kotlinxtras/curl/macosArm64/lib \
	-L/usr/local/kotlinxtras/libs/curl/macosArm64/lib 
libraryPaths.macos_arm64 = build/kotlinxtras/curl/macosArm64/lib \
	/usr/local/kotlinxtras/libs/curl/macosArm64/lib 
compilerOpts.macos_x64 = -Ibuild/kotlinxtras/curl/macosX64/include/curl \
	-I/usr/local/kotlinxtras/libs/curl/macosX64/include/curl 
linkerOpts.macos_x64 = -Lbuild/kotlinxtras/curl/macosX64/lib \
	-L/usr/local/kotlinxtras/libs/curl/macosX64/lib 
libraryPaths.macos_x64 = build/kotlinxtras/curl/macosX64/lib \
	/usr/local/kotlinxtras/libs/curl/macosX64/lib 
compilerOpts.mingw_x64 = -Ibuild/kotlinxtras/curl/mingwX64/include/curl \
	-I/usr/local/kotlinxtras/libs/curl/mingwX64/include/curl 
linkerOpts.mingw_x64 = -Lbuild/kotlinxtras/curl/mingwX64/lib \
	-L/usr/local/kotlinxtras/libs/curl/mingwX64/lib 
libraryPaths.mingw_x64 = build/kotlinxtras/curl/mingwX64/lib \
	/usr/local/kotlinxtras/libs/curl/mingwX64/lib 
